                    <m:mrow>
                      <m:mrow>
                        <m:mo stretchy="false">)</m:mo>
                        <m:mo stretchy="false">∗</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:mo stretchy="false">∂</m:mo>
                            <m:mfenced open="(" close=")">
                              <m:mrow>
                                <m:mi>g</m:mi>
                                <m:mo stretchy="false">(</m:mo>
                                <m:mi>t</m:mi>
                                <m:mo stretchy="false">)</m:mo>
                              </m:mrow>
                            </m:mfenced>
                          </m:mrow>
