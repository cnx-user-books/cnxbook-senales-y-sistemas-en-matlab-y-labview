                            <m:mo stretchy="false">(</m:mo>
                            <m:mi>t</m:mi>
                            <m:mo stretchy="false">)</m:mo>
                          </m:mrow>
                        </m:mfenced>
                      </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mrow>
                        <m:mfenced open="[" close="]">
                          <m:mrow>
                            <m:mi>f</m:mi>
                            <m:mo stretchy="false">(</m:mo>
                            <m:mi>t</m:mi>
                            <m:mrow>
                              <m:mo stretchy="false">)</m:mo>
                              <m:mo stretchy="false">∗</m:mo>
                              <m:mi>g</m:mi>
                            </m:mrow>
                            <m:mo stretchy="false">(</m:mo>
                            <m:mi>t</m:mi>
                            <m:mo stretchy="false">)</m:mo>
                          </m:mrow>
                        </m:mfenced>
                        <m:mo stretchy="false">∗</m:mo>
                        <m:mi>h</m:mi>
                      </m:mrow>
                    </m:mrow>
                    <m:mo stretchy="false">(</m:mo>
                    <m:mi>t</m:mi>
                    <m:mo stretchy="false">)</m:mo>
                  </m:mrow>
                </m:mrow>
              </m:mstyle>
              <m:mrow/>
            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{f \( t \)  *  left [g \( t \)  * h \( t \)  right ]= left [f \( t \)  * g \( t \)  right ] * h \( t \) } {}</m:annotation>
          </m:semantics>
        </m:math></equation><para id="id1165459854743">Propiedad distributiva:</para>
